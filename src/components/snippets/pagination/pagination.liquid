{%- if paginate.pages > 0 -%}
  <nav class="flex justify-center gap-1 empty:!hidden">
    {%- for part in paginate.parts -%}
      {%- if part.is_link -%}
        <a href="{{ part.url }}" class="flex items-center justify-center w-10 h-10 py-2 px-1 text-sm leading-2 font-medium rounded-full bg-[#ebeaeb] text-primary ">{{ part.title }}</a>
      {%- else -%}
        <span class="flex items-center justify-center w-10 h-10 py-2 px-1 text-sm leading-2 font-medium rounded-full text-primary{% if part.title == paginate.current_page %} bg-border{% else %} bg-[#ebeaeb]{% endif %}">{{ part.title }}</span>
      {%- endif -%}
    {%- endfor -%}
  </nav>
{%- endif -%}